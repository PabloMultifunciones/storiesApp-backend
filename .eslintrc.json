{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"],
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "prefer-template": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-await-in-loop": 0,
        "import/no-extraneous-dependencies": 0
    }
}
